/*!-----------------------------------------------------------------
	.header - модуль шапка сайта
 -----------------------------------------------------------------*/
.header
	background-color #fff
	position fixed
	width 100%
	height 100px
	z-index 100
	~/__wrap
		height 100px
		display flex
		align-items center
		justify-content space-between
	~/__logo
		& > svg
			width: 140px
			height: 60px

